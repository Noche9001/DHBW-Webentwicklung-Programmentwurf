+// https://www.youtube.com/watch?v=_Z9Axfh6AEU
+const app = require('express')();
+const appWs = require('express-ws')(app);
+
+let conns = 0;
+
+app.ws("/echo", ws => {
+    conns++;
+    console.log(conns)
+    ws.on('message', msg => {
+        console.log("Sent: " + msg)
+        ws.send(msg);
+    });
+    ws.on('close', () => {
+        conns--;
+        console.log(conns)
+    });
+});
+
+app.listen(1337, () => console.log('Started'))
